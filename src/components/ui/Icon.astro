---
import { IconsDict } from '@libs/data/icons';

interface Props {
    class?: string;
    label?: string;
    name: keyof typeof IconsDict;
}
const { class: className = "", label, name } = Astro.props;
---

<svg
    aria-label={label}
    aria-hidden="true"
    class:list={[className]}
    viewBox={IconsDict[name].viewbox}
    set:html={IconsDict[name].content}
/>